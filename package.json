{
    "name": "gravity-sim",
    "version": "0.0.0",
    "private": true,
    "type": "module",
    "engines": {
        "node": "^20.19.0 || >=22.12.0"
    },
    "scripts": {
        "dev": "vite",
        "build": "run-s build-rust build-vite",
        "build-vite": "run-p type-check \"build-only {@}\" --",
        "build-only": "vite build",
        "build-rust": "cd rust && wasm-pack build --release --target web && node post-process.js",
        "build-rust-dev": "cd rust && wasm-pack build --debug --target web && node post-process.js",
        "preview": "vite preview",
        "type-check": "vue-tsc --build"
    },
    "dependencies": {
        "@vueuse/core": "^14.1.0",
        "npm-run-all2": "^8.0.4",
        "pinia": "^3.0.4",
        "rust": "file:rust/pkg",
        "vite-plugin-top-level-await": "^1.6.0",
        "vue": "^3.5.22"
    },
    "devDependencies": {
        "@mdi/js": "^7.4.47",
        "@tsconfig/node22": "^22.0.2",
        "@types/node": "^22.18.11",
        "@vitejs/plugin-vue": "^6.0.1",
        "@vue/tsconfig": "^0.8.1",
        "sass-embedded": "^1.93.3",
        "typescript": "~5.9.0",
        "vite": "^7.1.11",
        "vite-plugin-vue-devtools": "^8.0.3",
        "vue-tsc": "^3.1.1"
    }
}
